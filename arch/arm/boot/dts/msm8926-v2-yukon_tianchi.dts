/dts-v1/;
/include/ "tianchi/msm8926-v2-tianchi.dtsi"
/include/ "tianchi/msm8226-tianchi_dsds.dtsi"
/include/ "tianchi/dsi-tianchi_dsds-panel.dtsi"
/include/ "tianchi/msm8226-camera-sensor-tianchi.dtsi"

/ {
	model = "SOMC TIANCHI";
	compatible = "somc,tianchi", "qcom,msm8926-mtp", "qcom,msm8926", "qcom,mtp";
	qcom,board-id = <8 0>;
};

&smsc_hub {
	status = "ok";
	smsc,model-id = <3502>;
	smsc,reset-gpio = <&msmgpio 114 0x00>;
	smsc,int-gpio = <&msmgpio 9 0x00>;
	smsc,xo-clk-gpio = <&msmgpio 8 0x00>;
	hub-int-supply = <&pm8226_l6>;
};

&soc {
        qcom,mdss_dsi_panel {
                status = "ok";
        };
};

&bu52031nvx_hall_ic {
	status = "ok";
};

&mdss_mdp {
	qcom,mdss-pref-prim-intf = "dsi";
};

&mdss_dsi0 {
	qcom,dsi-pref-prim-pan = <&dsi_novatek_jdi>;
};
